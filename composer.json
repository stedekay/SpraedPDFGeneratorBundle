{
    "name":        "spraed/pdf-generator-bundle",
    "type":        "symfony-bundle",
    "description": "This bundle creates (multiple) PDFs in Symfony from Twig/HTML templates.",
    "keywords":    ["spraed", "pdf", "generator", "bundle"],
    "homepage":    "https://github.com/stedekay/SpraedPDFGeneratorBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "Stefan Blanke"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/stedekay/SpraedPDFGeneratorBundle/contributors"
        }
    ],

    "require": {
        "php":                      "^8.0",
        "symfony/framework-bundle": "^6.0.4||^7.0||^8.0",
        "matthiasnoback/symfony-config-test": "^4.2.1"
    },

    "autoload": {
        "psr-4": {
            "Spraed\\PDFGeneratorBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spraed\\PDFGeneratorBundle\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    }
}
